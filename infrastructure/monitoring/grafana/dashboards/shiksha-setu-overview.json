{
  "dashboard": {
    "title": "ShikshaSetu - Application Overview",
    "tags": ["shiksha-setu", "education", "monitoring"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"shiksha-setu\"}[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"shiksha-setu\"}[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Response Time Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Active Requests",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "http_requests_in_progress{job=\"shiksha-setu\"}",
            "legendFormat": "Active Requests",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"shiksha-setu\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"shiksha-setu\",status=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Success Rate",
        "type": "singlestat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"shiksha-setu\",status!~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"shiksha-setu\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "format": "percent",
        "thresholds": "95,99",
        "colors": ["#d44a3a", "#e0b400", "#299c46"]
      },
      {
        "id": 6,
        "title": "Database Query Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket{job=\"shiksha-setu\"}[5m]))",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Active Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "targets": [
          {
            "expr": "db_connections_active{job=\"shiksha-setu\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(cache_hits_total{job=\"shiksha-setu\"}[5m]) / (rate(cache_hits_total{job=\"shiksha-setu\"}[5m]) + rate(cache_misses_total{job=\"shiksha-setu\"}[5m])) * 100",
            "legendFormat": "{{cache_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Cache Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "rate(cache_hits_total{job=\"shiksha-setu\"}[5m])",
            "legendFormat": "Hits - {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(cache_misses_total{job=\"shiksha-setu\"}[5m])",
            "legendFormat": "Misses - {{cache_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Pipeline Processing Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pipeline_processing_duration_seconds_bucket{job=\"shiksha-setu\"}[5m]))",
            "legendFormat": "{{stage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Pipeline Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "targets": [
          {
            "expr": "rate(pipeline_errors_total{job=\"shiksha-setu\"}[5m])",
            "legendFormat": "{{stage}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Model Inference Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_inference_duration_seconds_bucket{job=\"shiksha-setu\"}[5m]))",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Model Request Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 38},
        "targets": [
          {
            "expr": "rate(model_requests_total{job=\"shiksha-setu\",status=\"success\"}[5m]) / rate(model_requests_total{job=\"shiksha-setu\"}[5m]) * 100",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "Content Validation Checks",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 46},
        "targets": [
          {
            "expr": "rate(validation_checks_total{job=\"shiksha-setu\"}[5m])",
            "legendFormat": "{{check_type}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Checks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 15,
        "title": "Validation Score Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 46},
        "targets": [
          {
            "expr": "rate(validation_score_bucket{job=\"shiksha-setu\"}[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "yAxis": {"format": "short"},
        "cards": {"cardPadding": 2}
      }
    ],
    "refresh": "30s",
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
